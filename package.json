{
  "name": "bytelock-fullstack",
  "version": "1.0.0",
  "description": "ByteLock - Secure personal data management",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd Api && npm run dev",
    "dev:frontend": "cd Client/vite-project && npm run dev",
    "build": "cd Api && npm run build",
    "deploy": "npm run build && cd Api && npm start",
    "install:all": "npm install && cd Api && npm install && cd ../Client/vite-project && npm install",
    "postinstall": "cd Api && npm install --only=production && cd ../Client/vite-project && npm install",
    "start": "cd Api && npm start"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "@react-oauth/google": "^0.12.1",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.11",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  },
  "keywords": [
    "security",
    "password-manager",
    "finance",
    "encryption"
  ],
  "author": "",
  "license": "ISC"
}
